<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pv239_android">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:theme="@style/SecondAppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".model.Details"
            android:label="Details">
        </activity>
        <activity
            android:name=".EditNotesActivity"
            android:label="Notes"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".NewEventActivity"
            android:label="New event">
        </activity>
        <activity
            android:name=".CalendarActivity"
            android:label="Calendar">
        </activity>
        <activity
            android:name=".HistoryActivity"
            android:label="History of events">
        </activity>
    </application>

</manifest>